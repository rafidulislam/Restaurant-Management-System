@model Orders
<div class="row">
    <div class="col-md-4">
        <div asp-validation-summary="All">

        </div>
        <form asp-action="CheckOut" method="post" class="form-horizontal" role="form">
            

            <div class="form-group">
                <label asp-for="Name" class="col-md-2 control-label"></label>
                <div class="col-md-5">
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger">
                    </span>
                </div>
            </div>

            <div class="form-group">
                <div class="btn-group col-md-offset-2 col-md-8">
                    <input class="btn btn-success" type="submit" value="Complete order" />
                    @*<input class="btn btn-default" type="submit" value="Return " />*@
                </div>
            </div>

        </form>

    </div>

</div>